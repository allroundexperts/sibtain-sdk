  "keywords": ["The lord of the rings", "TLOR", "SDK", "OneAPI"],
  "author": "Sibtain Ali",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "fetch-mock-jest": "^1.5.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.7",
    "reflect-metadata": "^0.1.13",
